{
  "name": "documentviewer",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "nx",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points",
    "nx": "nx",
    "build:ngx-doc-viewer": "nx build ngx-doc-viewer --prod",
    "build:react-documents": "rimraf ./dist/libs/react-documents && rollup -c",
    "publish:react-documents": "npm run build:react-documents && npm publish ./dist/libs/react-documents --tag latest",
    "publish:ngx-doc-viewer": "npm run build:ngx-doc-viewer  && npm publish ./dist/libs/ngx-doc-viewer --tag latest",
    "publish-next:ngx-doc-viewer": "npm run build:ngx-doc-viewer && npm publish ./dist/libs/ngx-doc-viewer --tag next",
    "start:demo:angular": "nx serve demo-angular --port=4200",
    "start:demo:react": "nx serve demo-react --port=4201",
    "deploy:demo:angular": "firebase use angular-doc-viewer && firebase deploy --only hosting:demo-angular",
    "deploy:demo:react": "firebase use angular-doc-viewer && firebase deploy --only hosting:demo-react",
    "prepare-build": "node tools/scripts/prepare-build.ts ngx-doc-viewer",
    "lint": "nx workspace-lint && ng lint",
    "format:write": "nx format:write",
    "update": "nx migrate latest",
    "help": "nx help"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^11.2.0",
    "@angular/common": "^11.2.0",
    "@angular/compiler": "^11.2.0",
    "@angular/core": "^11.2.0",
    "@angular/forms": "^11.2.0",
    "@angular/platform-browser": "^11.2.0",
    "@angular/platform-browser-dynamic": "^11.2.0",
    "@angular/router": "^11.2.0",
    "@nrwl/angular": "11.5.1",
    "core-js": "^3.6.5",
    "document-register-element": "1.13.1",
    "rxjs": "~6.6.3",
    "tslib": "^2.0.0",
    "zone.js": "^0.10.2"
  },
  "peerDependencies": {
    "react": "17.0.1",
    "react-dom": "17.0.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1102.0",
    "@angular-eslint/eslint-plugin": "~1.0.0",
    "@angular-eslint/eslint-plugin-template": "~1.0.0",
    "@angular-eslint/template-parser": "~1.0.0",
    "@angular/cli": "~11.0.0",
    "@angular/compiler-cli": "^11.2.0",
    "@angular/language-service": "^11.2.0",
    "@babel/core": "7.9.6",
    "@babel/preset-env": "7.9.6",
    "@babel/preset-react": "7.9.4",
    "@babel/preset-typescript": "7.9.0",
    "@nrwl/cli": "11.5.1",
    "@nrwl/eslint-plugin-nx": "11.5.1",
    "@nrwl/linter": "11.5.1",
    "@nrwl/react": "11.5.1",
    "@nrwl/tao": "11.5.1",
    "@nrwl/web": "11.5.1",
    "@nrwl/workspace": "11.5.1",
    "@testing-library/react": "11.1.2",
    "@types/node": "12.12.38",
    "@types/react": "16.9.56",
    "@types/react-dom": "16.9.9",
    "@typescript-eslint/eslint-plugin": "4.3.0",
    "@typescript-eslint/parser": "4.3.0",
    "@rollup/plugin-commonjs": "^17.1.0",
    "@rollup/plugin-typescript": "^8.2.0",
    "@rollup/plugin-node-resolve": "^11.2.0",
    "bootstrap": "4.6.0",
    "dotenv": "6.2.0",
    "eslint": "7.10.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-cypress": "^2.10.3",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-react": "7.21.5",
    "eslint-plugin-react-hooks": "4.2.0",
    "mammoth": "1.4.16",
    "rimraf": "^3.0.2",
    "ng-packagr": "^11.2.4",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "rollup": "^2.41.2",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-copy": "3.4.0",
    "prettier": "2.2.1",
    "ts-node": "~9.1.1",
    "typescript": "~4.0.3"
  }
